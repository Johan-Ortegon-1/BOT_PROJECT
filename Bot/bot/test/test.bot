define moverArriba('pasos) ->
	^ pasos;
end;

define moverAbajo('pasos) ->
	V pasos;
end;

define moverDerecha('pasos) ->
	> pasos;
end;

define moverIzquierda('pasos) ->
	< pasos;
end;

define moverDiagonal('pasos) ->
	while pasos > 0 ->
		> 1;
		V 1;
		pasos <- pasos - 1;
	end;
end;

define calculaPotencia('base, 'exp) ->
	if exp = 0 ->
		return 1;
	else
		return base*exp;
	end;
end;

moverDerecha(13);
P;
moverAbajo(10);
moverIzquierda(13);
moverArriba(10);

'resultado <- calculaPotencia(4, 3);
moverDiagonal(resultado);
